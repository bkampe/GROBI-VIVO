<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

    <profiles>
        <profile>
            <id>defaults</id>
            <properties>
                <app-name>vivo</app-name>

                <vivo-dir>/tib/app/vivo/data/vivo</vivo-dir>
                <tomcat-dir>/Program Files/Apache Software Foundation/Tomcat 9.0</tomcat-dir>
                <vivo-installer-dir>../project-installer</vivo-installer-dir>

                <default-theme>tenderfoot</default-theme>
            </properties>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>defaults</activeProfile>
    </activeProfiles>
</settings>
